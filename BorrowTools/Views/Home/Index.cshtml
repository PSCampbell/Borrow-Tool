
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<h4>Axe and Hammer</h4>
<div>
    <canvas id="myCanvas" width="200" height="200" style="border:1px solid #d3d3d3;">
        Your browser does not support the HTML5 canvas tag.
    </canvas>
    <script>
        var c = document.getElementById("myCanvas");
        var ctx = c.getContext("2d");
        ctx.beginPath();
        ctx.moveTo(10, 190);
        ctx.lineTo(10, 50);
        ctx.lineTo(20, 50);
        ctx.lineTo(20, 190);
        ctx.lineTo(10, 190);
        ctx.lineTo(10, 50);
        ctx.lineTo(70, 50);
        ctx.lineTo(50, 90);
        ctx.lineTo(20, 90);
        ctx.stroke();
    </script>
    <script>
        var c = document.getElementById("myCanvas");
        var ctx = c.getContext("2d");
        ctx.beginPath();
        ctx.moveTo(100, 10);
        ctx.lineTo(180, 10);
        ctx.lineTo(180, 50);
        ctx.lineTo(100, 50);
        ctx.lineTo(100, 10);
        ctx.moveTo(130, 50);
        ctx.lineTo(130, 150);
        ctx.lineTo(150, 150);
        ctx.lineTo(150, 50);
        ctx.stroke();
    </script>
</div>


